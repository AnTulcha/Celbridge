<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <!-- Load WinUI resources -->
        <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />

        <!-- Load Uno.UI.Toolkit resources -->
        <ToolkitResources xmlns="using:Uno.Toolkit.UI" />

        <!-- Add resource dictionaries here -->
        <ResourceDictionary Source="ms-appx:///Celbridge.UserInterface/Resources/Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <FontFamily x:Key="DevOpIconsFontFamily">ms-appx:///Celbridge.UserInterface/Assets/Fonts/devopicons.ttf#devopicons</FontFamily>
    <FontFamily x:Key="FileIconsFontFamily">ms-appx:///Celbridge.UserInterface/Assets/Fonts/file-icons.ttf#file-icons</FontFamily>
    <FontFamily x:Key="FontAwesomeFontFamily">ms-appx:///Celbridge.UserInterface/Assets/Fonts/fontawesome.ttf#fontawesome</FontFamily>
    <FontFamily x:Key="MFixxFontFamily">ms-appx:///Celbridge.UserInterface/Assets/Fonts/mfixx.ttf#mfixx</FontFamily>
    <FontFamily x:Key="OctIconsFontFamily">ms-appx:///Celbridge.UserInterface/Assets/Fonts/octicons.ttf#octicons</FontFamily>

</ResourceDictionary>
