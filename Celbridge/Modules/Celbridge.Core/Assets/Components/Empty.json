{
  "type": "object",
  "additionalProperties": false,

  "attributes": {
    "rootActivity": "None",
    "allowMultipleComponents": true
  },

  "properties": {
    "_componentType": {
      "type": "string",
      "const": ".Empty#1"
    },
    "comment": {
      "type": "string"
    }
  },

  "required": [
    "_componentType",
    "comment"
  ],

  "prototype": {
    "comment": ""
  },

  "form": [
    {
      "element": "TextBox",
      "header": "Comment",
      "textBinding": "/comment",
      "placeholder": "Enter a comment",
      "checkSpelling": false
    }
  ]
}
